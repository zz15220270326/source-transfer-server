<thead class="list-table-head">
  <tr class="list-table-row">
    <% for (let head of tableHead) { %>
      <th
        key="<%= head.key %>"
        data-head="<%= JSON.stringify(head) %>"
        class="list-table-row"
        width="<%= head.width %>"
        align="<%= head.align %>"
      >
        <%= head.label %>
      </th>
    <% } %>
  </tr>
</thead>